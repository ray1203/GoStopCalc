<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ray1203.gostopcalc">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_gostop"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_gostop_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HistoryActivity"></activity>
        <activity android:name=".RecordActivity" />
        <activity android:name="CalcShowActivity"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CalcActivity" />
    </application>

</manifest>